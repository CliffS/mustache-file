{
  "name": "mustache-file",
  "version": "2.2.0",
  "description": "Read mustache files and partials from disk and pass them to mustache",
  "main": "lib/mustache.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "coffee -c -o lib src/*.coffee",
    "prepare": "npm run-script compile",
    "watch": "cd $(npm prefix); coffee -w -o lib -c src &"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CliffS/mustache-file.git"
  },
  "bin": {
    "mustache": "./bin/command.js"
  },
  "keywords": [
    "mustache",
    "partial"
  ],
  "author": {
    "name": "Cliff Stanford",
    "email": "cliff@may.be",
    "url": "http://may.be/"
  },
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/CliffS/mustache-file/issues"
  },
  "homepage": "https://github.com/CliffS/mustache-file#readme",
  "dependencies": {
    "async": "^1.4.2",
    "minimist": "^1.2.0",
    "mustache": "^3.2.1",
    "pretty": "^2.0.0"
  }
}
